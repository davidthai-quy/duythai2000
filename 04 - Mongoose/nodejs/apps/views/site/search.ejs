<% include layout/header %>                
                <!--	List Product	-->
                <div class="products">
                    <div id="search-result">Kết quả tìm kiếm với sản phẩm <span>iPhone Xs Max 2 Sim - 256GB</span></div>
                    <div class="product-list card-deck">
                        <%for(key in data.products){%>
                            <div class="product-item card text-center">
                                <a href="/product/<%=data.products[key]._id%>"><img src="/static/images/products/<%=data.products[key].prd_image%>"></a>
                                <h4><a href="/product/<%=data.products[key]._id%>"><%=data.products[key].prd_name%></a></h4>
                                <p>Giá Bán: <span><%=data.products[key].prd_price%></span></p>
                            </div>
                        <%}%>
                    </div>
                </div>
                <!--	End List Product	-->
                
                <div id="pagination">
                    <ul class="pagination">
                        <li class="page-item"><a class="page-link" href="#">Trang trước</a></li>
                        <li class="page-item active"><a class="page-link" href="#">1</a></li>
                        <li class="page-item"><a class="page-link" href="#">2</a></li>
                        <li class="page-item"><a class="page-link" href="#">3</a></li>
                        <li class="page-item"><a class="page-link" href="#">Trang sau</a></li>
                    </ul> 
                </div>
            </div>
            
<% include layout/footer %>