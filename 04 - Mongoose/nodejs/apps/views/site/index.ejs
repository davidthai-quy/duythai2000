<% include layout/header %>               
                <!--	Feature Product	-->
                <div class="products">
                    <h3>Sản phẩm nổi bật</h3>
                    <%for(key in data.featuredProduct){
                        if(data.i == 0){%>
                    <div class="product-list card-deck"> 
                        <%}%>
                        <div class="product-item card text-center">
                            <a href="#"><img src="/static/images/products/<%=data.featuredProduct[key].prd_image%>"></a>
                            <h4><a href="#"><%=data.featuredProduct[key].prd_name%></a></h4>
                            <p>Giá Bán: <span><%=data.featuredProduct[key].prd_price%></span></p>
                        </div>
                        <%data.i++
                        if(data.i == 3){ data.i = 0 %>
                    </div>
                        <%}
                    }%>
                </div>
                <!--	End Feature Product	-->
                
                
                <!--	Latest Product	-->
                <div class="products">
                    <h3>Sản phẩm mới</h3>
                    <div class="product-list card-deck">
                        <%for(key in data.latestProduct){%>
                            <div class="product-item card text-center">
                                <a href="#"><img src="/static/images/products/<%=data.latestProduct[key].prd_image%>"></a>
                                <h4><a href="#"><%=data.latestProduct[key].prd_name%></a></h4>
                                <p>Giá Bán: <span><%=data.latestProduct[key].prd_price%></span></p>
                            </div>
                        <%}%>
                    </div>
                </div>
                <!--	End Latest Product	-->
                
            </div>
<% include layout/footer %>
