<% include layout/header %>               
                <!--	List Product	-->
                <div class="products">
                    <h3><%=data.categoryName[0].cat_name%> (hiện có <%=data.total%> sản phẩm)</h3>
                    <div class="product-list card-deck">
                        <%for(key in data.productList){%>
                        <div class="product-item card text-center">
                            <a href="/product/<%=data.productList[key]._id%>"><img src="/static/images/products/<%=data.productList[key].prd_image%>"></a>
                            <h4><a href="/product/<%=data.productList[key]._id%>"><%=data.productList[key].prd_name%></a></h4>
                            <p>Giá Bán: <span><%=data.productList[key].prd_price%></span></p>
                        </div>
                        <%}%>
                    </div>
                </div>
                <!--	End List Product	-->
                
                <div id="pagination">
                    <ul class="pagination">
                        <li class="page-item"><a class="page-link" href="#">Trang trước</a></li>
                        <li class="page-item active"><a class="page-link" href="#">1</a></li>
                        <li class="page-item"><a class="page-link" href="#">2</a></li>
                        <li class="page-item"><a class="page-link" href="#">3</a></li>
                        <li class="page-item"><a class="page-link" href="#">Trang sau</a></li>
                    </ul> 
                </div>
                
            </div>
<% include layout/footer %>